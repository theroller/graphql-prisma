{
  "name": "graphql-prisma",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "debug": "cross-env DOTENV_CONFIG_PATH=config/dev.env node -r esm --nolazy --inspect-brk=9229 src/index.js",
    "get-schema": "graphql get-schema -p prisma --dotenv config/dev.env",
    "lint": "eslint src",
    "start": "node -r esm src/index.js",
    "start:dev": "cross-env DOTENV_CONFIG_PATH=config/dev.env nodemon src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "James Roller, Jr. <og.readmore@gmail.com>",
  "dependencies": {
    "babel-eslint": "^10.0.3",
    "bcrypt": "^3.0.7",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^6.7.2",
    "esm": "^3.2.25",
    "graphql-cli": "^3.0.14",
    "graphql-yoga": "^1.18.3",
    "jsonwebtoken": "^8.5.1",
    "prisma": "^1.34.10",
    "prisma-binding": "^2.3.16"
  },
  "devDependencies": {
    "nodemon": "^2.0.1"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*"
    ],
    "execMap": {
      "js": "node -r esm"
    },
    "ext": "graphql,js,json",
    "watch": [
      "src"
    ],
    "verbose": false
  }
}
