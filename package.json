{
  "name": "graphql-prisma",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "debug": "cross-env DOTENV_CONFIG_PATH=config/dev.env node --nolazy --inspect-brk=9229 src/index.js",
    "get-schema": "graphql get-schema -p prisma --dotenv config/dev.env",
    "lint": "eslint src tests",
    "start": "node src/index.js",
    "start:dev": "cross-env DOTENV_CONFIG_PATH=config/dev.env nodemon src/index.js",
    "test": "cross-env DOTENV_CONFIG_PATH=config/test.env jest --runInBand"
  },
  "keywords": [],
  "author": "James Roller, Jr. <og.readmore@gmail.com>",
  "dependencies": {
    "apollo-boost": "^0.4.7",
    "babel-eslint": "^10.0.3",
    "bcrypt": "^3.0.7",
    "cross-env": "^6.0.3",
    "cross-fetch": "^3.0.4",
    "dotenv": "^8.2.0",
    "eslint": "^6.7.2",
    "graphql": "^14.5.8",
    "graphql-cli": "^3.0.14",
    "graphql-yoga": "^1.18.3",
    "jsonwebtoken": "^8.5.1",
    "prisma": "^1.34.10",
    "prisma-binding": "^2.3.16"
  },
  "devDependencies": {
    "jest": "^24.9.0",
    "nodemon": "^2.0.1"
  },
  "jest": {
    "globalSetup": "./tests/jest/globalSetup.js",
    "globalTeardown": "./tests/jest/globalTeardown.js",
    "verbose": true
  },
  "nodemonConfig": {
    "ignore": [
      "test/*"
    ],
    "ext": "graphql,js,json",
    "watch": [
      "src"
    ],
    "verbose": false
  }
}
